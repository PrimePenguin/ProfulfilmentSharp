<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Version>0.1.8</Version>
    <AssemblyName>ProfulfilmentSharp</AssemblyName>
    <RootNamespace>ProfulfilmentSharp</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <Description>ProfulfilmentSharp is a C# and .NET library that helps developers easily authenticate with and manage Profulfilment Warehouse.</Description>
    <Authors>Ajay Kumar</Authors>
    <Copyright>Prime Penguin</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/PrimePenguin/ProfulfilmentSharp/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/PrimePenguin/ProfulfilmentSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PrimePenguin/ProfulfilmentSharp</RepositoryUrl>
    <PackageTags>Profulfilment,warehouse</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/PrimePenguin/ProfulfilmentSharp/master/PrimePenguin.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>
</Project>
